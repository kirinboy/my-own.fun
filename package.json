{
  "name": "my-own.fun",
  "version": "1.2.0",
  "description": "Your personal AI assistant",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "pnpm build:hmr && (run-p wss build:watch)",
    "dev:firefox": "pnpm build:hmr && (run-p wss build:firefox:watch)",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.js,.tsx,.jsx",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier . --write",
    "intl:extract": "npx react-intl-universal-extract --cmd extract --source-path ./src --output-path ./src/locales/en-US.json --verbose",
    "prepare": "husky install"
  },
  "type": "module",
  "dependencies": {
    "@ant-design/icons": "5.3.7",
    "@emotion/css": "^11.13.5",
    "@uiw/react-md-editor": "4.0.4",
    "antd": "5.16.5",
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.11.3",
    "construct-style-sheets-polyfill": "3.1.0",
    "copy-to-clipboard": "3.3.3",
    "effect": "^3.14.8",
    "jotai": "2.8.3",
    "jquery": "3.7.1",
    "liquidjs": "^10.20.3",
    "lodash": "4.17.21",
    "mermaid": "11.4.0",
    "openai": "4.39.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-bootstrap": "2.10.2",
    "react-dom": "18.2.0",
    "react-draggable": "4.4.6",
    "react-intl-universal": "2.12.0",
    "react-markdown": "9.0.1",
    "react-syntax-highlighter": "15.5.0",
    "rehype-highlight": "6.0.0",
    "rehype-katex": "6.0.3",
    "rehype-rewrite": "^4.0.2",
    "remark-gfm": "4.0.0",
    "remark-mermaidjs": "4.1.1",
    "textarea-caret": "^3.1.0",
    "uuid": "11.0.3",
    "webextension-polyfill": "0.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-sucrase": "5.0.2",
    "@rollup/plugin-typescript": "11.1.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.0.0",
    "@types/chrome": "0.0.287",
    "@types/jquery": "3.5.32",
    "@types/lodash": "4.17.4",
    "@types/node": "20.8.10",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.18",
    "@types/react-syntax-highlighter": "15.5.13",
    "@types/textarea-caret": "^3.0.3",
    "@types/ws": "8.5.8",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.18.1",
    "@vitejs/plugin-react": "4.2.0",
    "chokidar": "3.5.3",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lint-staged": "15.2.0",
    "magic-string": "0.30.10",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "react-intl-universal-extract": "^1.0.8",
    "rollup": "4.3.0",
    "sass": "1.72.0",
    "ts-loader": "9.5.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "5.0.12",
    "vitest": "1.5.3",
    "ws": "8.14.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "packageManager": "pnpm@8.9.2"
}
